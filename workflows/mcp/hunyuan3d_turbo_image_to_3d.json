{
  "1": {
    "inputs": {
      "image": "PARAM_STR_IMAGE_PATH",
      "upload": "image"
    },
    "class_type": "LoadImage",
    "_meta": {"title": "Load Image"}
  },
  "2": {
    "inputs": {
      "image": ["1", 0]
    },
    "class_type": "ImageResize+",
    "_meta": {"title": "Resize to 518x518"}
  },
  "3": {
    "inputs": {
      "model": "hy3dgen/hunyuan3d-dit-v2-turbo-fp16.safetensors",
      "attention_mode": "sdpa"
    },
    "class_type": "Hy3DModelLoader",
    "_meta": {"title": "Load Hunyuan3D Turbo Model"}
  },
  "4": {
    "inputs": {
      "model": "hunyuan3d-paint-v2-0-turbo"
    },
    "class_type": "DownloadAndLoadHy3DPaintModel",
    "_meta": {"title": "Load Paint Model (Turbo)"}
  },
  "5": {
    "inputs": {
      "base": "base",
      "post_processing": true
    },
    "class_type": "TransparentBGSession+",
    "_meta": {"title": "Background Removal Session"}
  },
  "6": {
    "inputs": {
      "rembg_session": ["5", 0],
      "image": ["2", 0]
    },
    "class_type": "ImageRemoveBackground+",
    "_meta": {"title": "Remove Background"}
  },
  "7": {
    "inputs": {
      "pipeline": ["3", 0],
      "image": ["2", 0],
      "mask": ["6", 1],
      "guidance_scale": "PARAM_FLOAT_GUIDANCE_SCALE",
      "steps": "PARAM_INT_STEPS",
      "seed": "PARAM_INT_SEED",
      "scheduler": "ConsistencyFlowMatchEulerDiscreteScheduler"
    },
    "class_type": "Hy3DGenerateMesh",
    "_meta": {"title": "Generate Mesh Latents (Turbo)"}
  },
  "8": {
    "inputs": {
      "vae": ["3", 1],
      "latents": ["7", 0],
      "box_v": 1.01,
      "octree_resolution": "PARAM_INT_OCTREE_RESOLUTION",
      "num_chunks": 32000,
      "mc_level": 0,
      "mc_algo": "mc"
    },
    "class_type": "Hy3DVAEDecode",
    "_meta": {"title": "VAE Decode to Mesh"}
  },
  "9": {
    "inputs": {
      "trimesh": ["8", 0],
      "remove_floaters": true,
      "remove_degenerate_faces": true,
      "reduce_faces": true,
      "max_facenum": "PARAM_INT_MAX_FACES",
      "smooth_normals": false
    },
    "class_type": "Hy3DPostprocessMesh",
    "_meta": {"title": "Postprocess Mesh"}
  },
  "10": {
    "inputs": {
      "trimesh": ["9", 0],
      "filename_prefix": "3D/Hy3D_turbo_geometry",
      "file_format": "glb"
    },
    "class_type": "Hy3DExportMesh",
    "_meta": {"title": "Export Geometry (no texture)"}
  },
  "11": {
    "inputs": {
      "trimesh": ["9", 0]
    },
    "class_type": "Hy3DMeshUVWrap",
    "_meta": {"title": "UV Wrap Mesh"}
  },
  "12": {
    "inputs": {
      "camera_azimuths": "0, 90, 180, 270, 0, 180",
      "camera_elevations": "0, 0, 0, 0, 90, -90",
      "view_weights": "1, 0.1, 0.5, 0.1, 0.05, 0.05",
      "camera_distance": 1.45,
      "ortho_scale": 1.2
    },
    "class_type": "Hy3DCameraConfig",
    "_meta": {"title": "Camera Configuration"}
  },
  "13": {
    "inputs": {
      "trimesh": ["11", 0],
      "render_size": 1024,
      "texture_size": 1024,
      "camera_config": ["12", 0],
      "normal_space": "world"
    },
    "class_type": "Hy3DRenderMultiView",
    "_meta": {"title": "Render Multi-View"}
  },
  "14": {
    "inputs": {
      "pipeline": ["4", 0],
      "ref_image": ["2", 0],
      "normal_maps": ["13", 0],
      "position_maps": ["13", 1],
      "view_size": 512,
      "steps": 15,
      "seed": "PARAM_INT_SEED",
      "camera_config": ["12", 0]
    },
    "class_type": "Hy3DSampleMultiView",
    "_meta": {"title": "Sample Multi-View Textures (Turbo)"}
  },
  "15": {
    "inputs": {
      "images": ["14", 0],
      "renderer": ["13", 2],
      "camera_config": ["12", 0]
    },
    "class_type": "Hy3DBakeFromMultiview",
    "_meta": {"title": "Bake Texture from Multi-View"}
  },
  "16": {
    "inputs": {
      "texture": ["15", 0],
      "mask": ["15", 1],
      "renderer": ["15", 2]
    },
    "class_type": "Hy3DMeshVerticeInpaintTexture",
    "_meta": {"title": "Vertex Inpaint Texture"}
  },
  "17": {
    "inputs": {
      "texture": ["16", 0],
      "mask": ["16", 1],
      "inpaint_radius": 3,
      "method": "ns"
    },
    "class_type": "CV2InpaintTexture",
    "_meta": {"title": "CV2 Inpaint Texture"}
  },
  "18": {
    "inputs": {
      "texture": ["17", 0],
      "renderer": ["16", 2]
    },
    "class_type": "Hy3DApplyTexture",
    "_meta": {"title": "Apply Texture to Mesh"}
  },
  "19": {
    "inputs": {
      "trimesh": ["18", 0],
      "filename_prefix": "3D/Hy3D_turbo_textured",
      "file_format": "glb"
    },
    "class_type": "Hy3DExportMesh",
    "_meta": {"title": "Export Textured Mesh"}
  }
}
