{
  "1": {
    "inputs": {
      "ckpt_name": "flux1-dev-fp8.safetensors"
    },
    "class_type": "CheckpointLoaderSimple",
    "_meta": {"title": "Load Checkpoint"}
  },
  "2": {
    "inputs": {
      "lora_name": "style/PixelArtV3Flux.safetensors",
      "strength_model": "PARAM_FLOAT_LORA_STRENGTH",
      "strength_clip": "PARAM_FLOAT_LORA_STRENGTH",
      "model": ["1", 0],
      "clip": ["1", 1]
    },
    "class_type": "LoraLoader",
    "_meta": {"title": "Load Pixel Art LoRA"},
    "_defaults": {
      "PARAM_FLOAT_LORA_STRENGTH": 0.85
    }
  },
  "3": {
    "inputs": {
      "width": "PARAM_INT_WIDTH",
      "height": "PARAM_INT_HEIGHT",
      "batch_size": 1
    },
    "class_type": "EmptySD3LatentImage",
    "_meta": {"title": "Empty Latent Image"},
    "_defaults": {
      "PARAM_INT_WIDTH": 1024,
      "PARAM_INT_HEIGHT": 1024
    }
  },
  "4": {
    "inputs": {
      "text": "PARAM_PROMPT",
      "clip": ["2", 1]
    },
    "class_type": "CLIPTextEncode",
    "_meta": {"title": "Positive Prompt"}
  },
  "5": {
    "inputs": {
      "text": "PARAM_NEGATIVE_PROMPT",
      "clip": ["2", 1]
    },
    "class_type": "CLIPTextEncode",
    "_meta": {"title": "Negative Prompt"},
    "_defaults": {
      "PARAM_NEGATIVE_PROMPT": ""
    }
  },
  "6": {
    "inputs": {
      "seed": "PARAM_INT_SEED",
      "steps": "PARAM_INT_STEPS",
      "cfg": "PARAM_FLOAT_CFG",
      "sampler_name": "euler",
      "scheduler": "simple",
      "denoise": 1.0,
      "model": ["2", 0],
      "positive": ["4", 0],
      "negative": ["5", 0],
      "latent_image": ["3", 0]
    },
    "class_type": "KSampler",
    "_meta": {"title": "KSampler"},
    "_defaults": {
      "PARAM_INT_SEED": -1,
      "PARAM_INT_STEPS": 20,
      "PARAM_FLOAT_CFG": 1.0
    }
  },
  "7": {
    "inputs": {
      "samples": ["6", 0],
      "vae": ["1", 2]
    },
    "class_type": "VAEDecode",
    "_meta": {"title": "VAE Decode"}
  },
  "8": {
    "inputs": {
      "filename_prefix": "ComfyUI_PixelArt",
      "images": ["7", 0]
    },
    "class_type": "SaveImage",
    "_meta": {"title": "Save Image"}
  }
}
